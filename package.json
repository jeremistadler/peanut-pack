{
  "name": "peanut-pack",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Jeremi Stadler",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/peanut-pack.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "percentile": "^1.6.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2",
    "ts-node": "^10.7.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.17.12",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "jest": "^28.1.0"
  },
  "size-limit": [
    {
      "path": "dist/peanut-pack2.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/peanut-pack2.esm.js",
      "limit": "10 KB"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid"
  }
}