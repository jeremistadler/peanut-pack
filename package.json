{
  "name": "peanut-pack",
  "version": "0.0.7",
  "license": "MIT",
  "author": "Jeremi Stadler",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/peanut-pack.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "fuzz": "node -r esbuild-register src/scripts/fuzzTestCompression.ts",
    "fuzzLarge": "node -r esbuild-register src/scripts/fuzzTestCompressionLarge.ts"
  },
  "dependencies": {
    "percentile": "^1.6.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "esbuild-register": "^3.4.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "size-limit": [
    {
      "path": "dist/peanut-pack2.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/peanut-pack2.esm.js",
      "limit": "10 KB"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid"
  }
}